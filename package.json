{
  "name": "dexible-pot-lib",
  "version": "1.0.1",
  "description": "Library to interact with Dexible Proof of Trade conrtracts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buidlhub/dexible-pot-lib.git"
  },
  "author": "support@buidlhub.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean; tsc",
    "test": "jest",
    "do_publish": "npm publish --access=public"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/solidity": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/wallet": "^5.7.0",
    "@ethersproject/web": "^5.7.1",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^2.3.4",
    "rimraf": "^4.1.2",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "roots": [
      "src",
      "test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "bugs": {
    "url": "https://github.com/buidlhub/dexible-pot-lib/issues"
  },
  "homepage": "https://github.com/buidlhub/dexible-pot-lib#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "dexible",
    "proof-of-trade",
    "dxbl"
  ]
}
